<consulo-plugin>
	<id>consulo.aspnet</id>
	<name>ASP .NET 4</name>
	<version>SNAPSHOT</version>
	<vendor>consulo.io</vendor>
	<category>Web Frameworks</category>
	<depends>consulo.dotnet</depends>
	<experimental>true</experimental>
	<tags>
		<tag>web.framework</tag>
		<tag>web.server</tag>
		<tag>experimental</tag>
	</tags>

	<depends config-file="consulo.dotnet.microsoft.xml" optional="true">consulo.dotnet.microsoft</depends>
	<depends config-file="consulo.dotnet.mono.xml" optional="true">consulo.dotnet.mono</depends>

	<extensions defaultExtensionNs="com.intellij">
		<configurationType implementation="consulo.aspnet.module.run.AspNetConfigurationType" />
	</extensions>
</consulo-plugin>